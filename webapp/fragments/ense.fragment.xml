<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:smTable="sap.ui.comp.smarttable"
	xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <smTable:SmartTable
        id="smartTable4"
        entitySet="CRLICENSES"
        editable="{data>/isEditable}"
		app:useSmartField="true"
        tableBindingPath="to_CRLicenses"
        enableAutoBinding="true"
        initiallyVisibleFields="LicenseType,LicenseIssuer,ProfLicenseNumber,Country,State_Region,IssueDate,ExpirationDate"
    >
        <smTable:customToolbar>
            <Toolbar>
                <ToolbarSpacer />
                <Button
                    icon="sap-icon://add"
                    press="onAddTab4"
                />
                <Button
                    icon="sap-icon://delete"
                    press="ondel1"
                />
            </Toolbar>
        </smTable:customToolbar>
    </smTable:SmartTable>
</c:FragmentDefinition>
