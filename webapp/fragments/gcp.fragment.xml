<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:smTable="sap.ui.comp.smarttable"
	 xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <smTable:SmartTable
        id="smartTable5"
        entitySet="GCPTRAININGS"
        tableBindingPath="to_GCPTRAININGS"
        enableAutoBinding="true"
		app:useSmartField="true"
		editable="{data>/isEditable}"
        initiallyVisibleFields="TrainingProvider,TitleofTraining,Version,DateofCompleted,Status"
    >
        <smTable:customToolbar>
            <Toolbar>
                <ToolbarSpacer />
                <Button
                    icon="sap-icon://add"
                    press="onAddTab5"
                />
                <Button
                    icon="sap-icon://delete"
                    press="ondel1"
                />
            </Toolbar>
        </smTable:customToolbar>
    </smTable:SmartTable>
</c:FragmentDefinition>
