<!-- TrainingProvider,TitleofTraining,Version,DateofCompleted,Status -->
<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
    xmlns:smform="sap.ui.comp.smartform"
    xmlns:smfield="sap.ui.comp.smartfield"
>
<Dialog>
 <smform:SmartForm id="smartformAdd5" editable="true">
                                        <smform:Group>
                                            <smform:GroupElement>
                                                <smfield:SmartField
                                                    editable="false"
                                                    value="{SiteCode}"
                                                />
                                            </smform:GroupElement>
                                            <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{TrainingProvider}"
                                                />
                                            </smform:GroupElement>
                                            <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{TitleofTraining}"
                                                />
                                            </smform:GroupElement>
                                              <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{Version}"
                                                />
                                            </smform:GroupElement>
                                            <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{DateofCompleted}"
                                                />
                                            </smform:GroupElement>
                                             <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{Status}"
                                                />
                                            </smform:GroupElement>
                                        </smform:Group>
                                    </smform:SmartForm>
                                    <Toolbar >
                                     <ToolbarSpacer ></ToolbarSpacer>
                                    <Button text="save" press="onSave5"/>
                                    <Button text="cancel" press="onClose5"/>
                                    </Toolbar>
                                    </Dialog>
</c:FragmentDefinition>