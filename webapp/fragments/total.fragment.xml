<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:smTable="sap.ui.comp.smarttable"
	xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <smTable:SmartTable
        id="smartTable7"
        entitySet="totalcrexp"
		app:useSmartField="true"
        editable="{data>/isEditable}"
        tableBindingPath="to_TCREXP"
        enableAutoBinding="true"
        initiallyVisibleFields="therapeuticarea,subtherapeuticarea,sponsername,CROname,Numofcomstu,Numofongstu,startdate,enddate,Status"
    >
        <smTable:customToolbar>
            <Toolbar>
                <ToolbarSpacer />
                <Button
                    icon="sap-icon://add"
                    press="onAddTab7"
                />
                <Button
                    icon="sap-icon://delete"
                    press="ondel1"
                />
            </Toolbar>
        </smTable:customToolbar>
    </smTable:SmartTable>
</c:FragmentDefinition>
