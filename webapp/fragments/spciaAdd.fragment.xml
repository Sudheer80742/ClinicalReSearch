<!-- Specialities,SubSpecialities -->
<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
    xmlns:smform="sap.ui.comp.smartform"
    xmlns:smfield="sap.ui.comp.smartfield"
>
<Dialog>
 <smform:SmartForm id="smartformAdd6" editable="true">
                                        <smform:Group>
                                            <smform:GroupElement>
                                                <smfield:SmartField
                                                    editable="false"
                                                    value="{Sitecode}"
                                                />
                                            </smform:GroupElement>
                                            <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{Specialities}"
                                                />
                                            </smform:GroupElement>
                                            <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{SubSpecialities}"
                                                />
                                            </smform:GroupElement>
                                        </smform:Group>
                                    </smform:SmartForm>
                                    <Toolbar >
                                     <ToolbarSpacer ></ToolbarSpacer>
                                    <Button text="save" press="onSave6"/>
                                    <Button text="cancel" press="onClose6"/>
                                    </Toolbar>
                                    </Dialog>
</c:FragmentDefinition>