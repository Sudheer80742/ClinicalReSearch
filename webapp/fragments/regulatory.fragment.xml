<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:smTable="sap.ui.comp.smarttable"
	 xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <smTable:SmartTable
        id="smartTable8"
        entitySet="CRregulatoryinformation"
        editable="{data>/isEditable}"
		app:useSmartField="true"
        tableBindingPath="to_CRregulatoryInfo"
        enableAutoBinding="true"
        initiallyVisibleFields="DocCategory,DocName,Avialability,DocStatus,EndDate,Remarks,startdate,enddate,Status"
    >
        <smTable:customToolbar>
            <Toolbar>
                <ToolbarSpacer />
                <Button
                    icon="sap-icon://add"
                    press="onAddTab8"
                />
                <Button
                    icon="sap-icon://delete"
                    press="ondel1"
                />
            </Toolbar>
        </smTable:customToolbar>
    </smTable:SmartTable>
</c:FragmentDefinition>
