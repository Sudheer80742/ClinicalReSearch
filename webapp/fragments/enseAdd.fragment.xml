<!-- LicenseType,LicenseIssuer,ProfLicenseNumber,Country,State_Region,IssueDate,ExpirationDate -->
<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
    xmlns:smform="sap.ui.comp.smartform"
    xmlns:smfield="sap.ui.comp.smartfield"
>
<Dialog>
 <smform:SmartForm id="smartformAdd4" editable="true">
                                        <smform:Group>
                                            <smform:GroupElement>
                                                <smfield:SmartField
                                                    editable="false"
                                                    value="{SiteCode}"
                                                />
                                            </smform:GroupElement>
                                            <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{LicenseType}"
                                                />
                                            </smform:GroupElement>
                                            <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{LicenseIssuer}"
                                                />
                                            </smform:GroupElement>
                                              <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{ProfLicenseNumber}"
                                                />
                                            </smform:GroupElement>
                                            <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{Country}"
                                                />
                                            </smform:GroupElement>
                                             <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{State_Region}"
                                                />
                                            </smform:GroupElement>
                                            <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{IssueDate}"
                                                />
                                            </smform:GroupElement>
                                            <smform:GroupElement>
                                                <smfield:SmartField
                                                    value="{ExpirationDate}"
                                                />
                                            </smform:GroupElement>
                                        </smform:Group>
                                    </smform:SmartForm>
                                    <Toolbar >
                                     <ToolbarSpacer ></ToolbarSpacer>
                                    <Button text="save" press="onSave4"/>
                                    <Button text="cancel" press="onClose4"/>
                                    </Toolbar>
                                    </Dialog>
</c:FragmentDefinition>