<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:smform="sap.ui.comp.smartform"
    xmlns:smfield="sap.ui.comp.smartfield"
>
    <smform:SmartForm id="smartform" editable="{data>/isEditable}">
        <smform:Group>
            <smform:GroupElement>
                <smfield:SmartField value="{SiteCode}" />
            </smform:GroupElement>
            <smform:GroupElement>
                <smfield:SmartField value="{SiteName}" />
            </smform:GroupElement>
            <smform:GroupElement>
                <smfield:SmartField value="{CRCode}" />
            </smform:GroupElement>
        </smform:Group>
        <smform:Group>
            <smform:GroupElement>
                <smfield:SmartField value="{Salutation}" />
            </smform:GroupElement>
            <smform:GroupElement>
                <smfield:SmartField value="{Job_ProfessionalTitle}" />
            </smform:GroupElement>
            <smform:GroupElement>
                <smfield:SmartField value="{CRName}" />
            </smform:GroupElement>
        </smform:Group>
        <smform:Group>
            <smform:GroupElement>
                <smfield:SmartField value="{ClincialRhRole}" />
            </smform:GroupElement>
            <smform:GroupElement>
                <smfield:SmartField value="{Specialities}" />
            </smform:GroupElement>
            <smform:GroupElement>
                <smfield:SmartField value="{CRExperience}" />
            </smform:GroupElement>
            </smform:Group>
            <smform:Group>
            <smform:GroupElement>
                <smfield:SmartField value="{Cerofgoodclinicalprac}" />
            </smform:GroupElement>
            <smform:GroupElement>
                <smfield:SmartField value="{CVAvailable}" />
            </smform:GroupElement>
            <smform:GroupElement>
                <smfield:SmartField value="{Status}" />
            </smform:GroupElement>
        </smform:Group>
    </smform:SmartForm>
</c:FragmentDefinition>